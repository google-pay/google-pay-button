{
  "private": true,
  "name": "google-pay-button",
  "version": "0.3.0",
  "description": "Google Pay button component",
  "main": "dist/GooglePayButton.js",
  "types": "dist/GooglePayButton.d.ts",
  "scripts": {
    "lint": "eslint .",
    "build": "npm run lint && scripts/build.sh",
    "build:all": "npm run build && npm run build:examples",
    "build:examples": "npm run build:example:angular && npm run build:example:react",
    "build:example:angular": "cd examples/angular && npm install && npm run build",
    "build:example:react": "cd examples/react && npm install && npm run build",
    "test": "jest",
    "prepublishOnly": "npm run test && npm run build",
    "tag": "git tag -f \"v$(node scripts/version.js)\"",
    "publish:all": "scripts/publish.sh"
  },
  "repository": {
    "url": "https://github.com/google-pay/google-pay-button"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "author": "socsieng@gmail.com",
  "license": "Apache-2.0",
  "dependencies": {
    "@types/googlepay": "^0.4.0"
  },
  "peerDependencies": {
    "@types/react": ">=16.0.0",
    "@types/react-dom": ">=16.0.0",
    "react": ">=16.3.0",
    "react-dom": ">=16.3.0"
  },
  "devDependencies": {
    "@rollup/plugin-json": "^4.0.3",
    "@types/jest": "^24.0.23",
    "@types/react": ">=16.0.0",
    "@types/react-dom": ">=16.0.0",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "prettier": "^2.0.5",
    "react": ">=16.3.0",
    "react-dom": ">=16.3.0",
    "rimraf": "^3.0.0",
    "rollup": "^2.10.0",
    "rollup-plugin-dts": "^1.4.3",
    "rollup-plugin-typescript2": "^0.27.1",
    "ts-jest": "^25.5.1",
    "typescript": "^3.9.2"
  }
}
