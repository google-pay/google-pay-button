{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "outDir": "/dist-tsc/lib",
    "declarationMap": true,
    "target": "es2015",
    "declaration": true,
    "inlineSources": true,
    "lib": ["dom", "es2018"],
    "rootDirs": ["./", "../../"],
    "baseUrl": "../"
  },
  "angularCompilerOptions": {
    "skipTemplateCodegen": true,
    "strictMetadataEmit": true,
    "enableResourceInlining": true
  },
  "exclude": ["src/test.ts", "**/*.spec.ts"]
}
